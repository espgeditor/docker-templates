<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>espgeditor-beta</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>espgeditor-beta</Overview>
  <Description>
        espgeditor-beta
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/espg/espgeditor-beta/</Registry>
  <Repository>espg/espgeditor-beta</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>58501</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>58081</HostPort>
        <ContainerPort>8000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
      <Variable>
        <Value>99</Value>
        <Name>USER_ID</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>100</Value>
        <Name>GROUP_ID</Name>
        <Mode/>
      </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/espgeditor-beta</HostDir>
      <ContainerDir>/app/espg-server/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:58081]/</WebUI>
  <Banner></Banner>
  <Icon>http://espg.xyz/espgeditordocker.png</Icon>
</Container>
