<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>ipbs</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>ipbs</Overview>
  <Description>
        ipbs
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/espg/ipbs/</Registry>
  <Repository>espg/ipbs</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>60900</HostPort>
        <ContainerPort>5900</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>60808</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
      <Variable>
        <Value>99</Value>
        <Name>USER_ID</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>100</Value>
        <Name>GROUP_ID</Name>
        <Mode/>
      </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/ipbs</HostDir>
      <ContainerDir>/app/</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:60808]/vnc.html?autoconnect=true</WebUI>
  <Banner></Banner>
  <Icon></Icon>
</Container>
