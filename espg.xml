<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>espg</Name>
  <Beta>False</Beta>
  <Category>Tools:Utilities</Category>
  <Overview>espg</Overview>
  <Description>
        espg
  </Description>
  <Change></Change>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/espg/espg/</Registry>
  <Repository>espg/espg</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>55580</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>55500</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
    </Networking>
    <Environment>
      <Variable>
        <Value>99</Value>
        <Name>USER_ID</Name>
        <Mode/>
      </Variable>
      <Variable>
        <Value>100</Value>
        <Name>GROUP_ID</Name>
        <Mode/>
      </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/espg</HostDir>
      <ContainerDir>/app/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:55580]/</WebUI>
  <Banner></Banner>
  <Icon>http://espg.xyz/espg.png</Icon>
</Container>
